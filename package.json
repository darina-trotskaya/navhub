{
  "name": "edu-env",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel start:{dev,themekit}",
    "clean": "rm -rf build && rm -rf cache && rm -rf .next",
    "build": "next build",
    "build:themekit": "npm run themekit",
    "start": "npm run start:prod",
    "start:dev": "next dev",
    "start:prod": "next start",
    "start:themekit": "yarn themekit:watch",
    "lint": "npm-run-all lint:*",
    "lint:scripts": "eslint './**/*.{js,ts,tsx}'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:types": "tsc --noemit",
    "fix": "npm-run-all fix:*",
    "fix:scripts": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
    "fix:css": "stylelint 'src/**/*.css' --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "deps": "yarn i --no-package-lock",
    "themekit": "themekit build",
    "themekit:watch": "npm run themekit -- -w"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "lint:scripts",
    "*.css": "lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint"
    }
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.9",
    "@bem-react/classnames": "^1.3.9",
    "next": "10.0.7",
    "postcss": "^8.2.6",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@yandex/themekit": "^1.6.6",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^8.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^4.1.5",
    "typescript-plugin-css-modules": "3.1.0"
  }
}
